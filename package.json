{
  "name": "@elearn/react-quiz",
  "version": "1.0.0",
  "description": "React Component rendering quiz",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rm -rf dist/",
    "build": "babel src --out-dir dist --source-maps",
