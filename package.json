{
  "name": "react-quiz",
  "version": "0.1.0",
  "description": "React Component rendering quiz, not publish to npm",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rm -rf dist/",
    "build": "babel src --out-dir dist --source-maps",
    "gen-test": "babel example/quiz-gen.js --out-file dist/quiz-gen.js --source-maps && node dist/quiz-gen.js",
    "start": "webpack-dev-server --config webpack.dev.config.js  --port 3200 --host 0.0.0.0 --watch-poll"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/duongtdn/react-quiz.git"
  },
  "keywords": [
    "react",
    "quiz"
  ],
  "author": "duongtdn",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/duongtdn/react-quiz/issues"
  },
  "homepage": "https://github.com/duongtdn/react-quiz#readme",
  "dependencies": {
    "react": "^16.8.3",
    "react-dom": "^16.8.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.2",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.5",
    "webpack": "^4.29.5",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.1.14"
  }
}
